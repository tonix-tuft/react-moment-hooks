{
  "name": "react-moment-hooks",
  "version": "2.1.0",
  "description": "React hooks for dealing with the Moment.js library.",
  "author": "Anton Bagdatyev (Tonix)",
  "license": "MIT",
  "repository": "tonix-tuft/react-moment-hooks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "keywords": [
    "react",
    "moment",
    "date",
    "time",
    "format",
    "i18n"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "mkdir-build": "mkdir -p ./dist",
    "prepare": "npm run mkdir-build && npm run clear-build && npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "clear-build": "find ./dist ! -name '.gitignore' -type f -exec rm -f {} +",
    "build-module": "npm run mkdir-build && cross-env BABEL_ENV=module ./node_modules/.bin/babel ./src --out-dir ./module --source-maps --copy-files",
    "watch-build-module": "npm run build-module && ./node_modules/.bin/onchange './src/**' -- npm run build-module",
    "publish-patch": "npm run prepare && npm version patch && npm publish && git push && git push --tags",
    "publish-minor": "npm run prepare && npm version minor && npm publish && git push && git push --tags",
    "publish-major": "npm run prepare && npm version major && npm publish && git push && git push --tags"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "moment": "^2.0.0",
    "moment-utl": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.7",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.1",
    "@babel/traverse": "^7.9.5",
    "@babel/types": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-url": "^4.0.2",
    "@svgr/rollup": "^4.3.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "moment": "^2.24.0",
    "moment-utl": "^2.1.0",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.8.1",
    "typescript": "^3.7.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "immutable-linked-ordered-map": "^1.7.0",
    "react-js-utl": "^1.39.0"
  }
}
